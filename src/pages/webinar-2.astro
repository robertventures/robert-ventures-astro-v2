---
import BaseHead from "../components/BaseHead.astro";
import Header from "../components/Header.astro";
import NotificationBar from "../components/NotificationBar.astro";

import HeroboxWebinar10WithLogo from "../sections/HeroboxWebinar10WithLogo.astro";
import WhyNowWebinar from "../sections/WhyNowWebinar.astro";
import JoeRobertWebinar from "../sections/JoeRobertWebinar.astro";
import Testimonials from "../sections/Testimonials.astro";

import FooterNew from "../components/FooterNew.astro";
import HowItWorksV2 from "../sections/HowItWorksV2.astro";
import InvestmentOptions from "../sections/InvestmentOptions.astro";

const pageTitle = "Free Investor Webinar - Fixed Income Bonds | Robert Ventures";
const pageDescription =
    "Review our investor webinar presentation to learn more about our 8-10% annual interest rate corporate bonds.";


export const prerender = false

---

<!doctype html>
<html lang="en">
    <head>
        <BaseHead title={pageTitle} description={pageDescription} />
        <meta property="og:url" content="https://robertventures.com/webinar" />
        <meta name="robots" content="noindex, nofollow" />
    </head>
    <body>
        <Header />
        <HeroboxWebinar10WithLogo />
        <InvestmentOptions/>
        <HowItWorksV2/>
        <WhyNowWebinar />
        <JoeRobertWebinar />
        <Testimonials />
        <FooterNew />
        <NotificationBar />
    </body>
</html>
